datasource db {
  provider = "mysql"
  url     = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         Int       @id @default(autoincrement())
  email      String
  created_at DateTime
  update_at  DateTime
}

model Item {
  id      BigInt      @id @default(autoincrement())
  user_id Int
  amount Int
}